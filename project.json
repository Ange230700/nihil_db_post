{
  "name": "db-post",
  "root": "post",
  "projectType": "library",
  "implicitDependencies": ["db-shared"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run build", "cwd": "post" },
      "dependsOn": ["prisma:generate"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run test", "cwd": "post" }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run clean", "cwd": "post" }
    },
    "prisma:generate": {
      "executor": "nx:run-commands",
      "options": { "command": "prisma generate", "cwd": "post" }
    },
    "prisma:db:seed": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run prisma:db:seed", "cwd": "post" }
    },
    "prisma:db:push": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run prisma:db:push", "cwd": "post" }
    },
    "prisma:migrate:reset": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run prisma:migrate:reset", "cwd": "post" }
    },
    "prisma:migrate:reset:force": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:migrate:reset:force",
        "cwd": "post"
      }
    },
    "publish:package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run publish:package",
        "cwd": "post",
        "env": { "NODE_ENV": "prod" }
      },
      "dependsOn": ["build"]
    }
  }
}
