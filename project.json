{
  "name": "@nihil_databases/post",
  "root": "post",
  "projectType": "library",
  "implicitDependencies": ["@nihil_databases/shared"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b",
        "cwd": "post"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "post"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run clean",
        "cwd": "post"
      }
    },
    "prisma:db:seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:db:seed",
        "cwd": "post"
      }
    },
    "prisma:db:push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:db:push",
        "cwd": "post"
      }
    },
    "prisma:migrate:reset": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:migrate:reset",
        "cwd": "post"
      }
    },
    "prisma:migrate:reset:force": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:migrate:reset:force",
        "cwd": "post"
      }
    }
  }
}
